<?xml version="1.0"?>
<launch>

    <include file="$(find thmos_bringup)/launch/load_robot_description_thmos.launch" />
  
    <node name="walk_rviz" pkg="rviz" type="rviz" args="-d $(find bitbots_quintic_walk)/config/walk.rviz" />
  
    <node name="joint_goal_viz" pkg="thmos_bringup" type="motor_goals_viz_helper_thmos.py" output="screen" args="--all" />
  
    <node name="dynamic_kick" pkg="bitbots_dynamic_kick" type="KickNode" output="screen">
        <rosparam command="load" file="$(find bitbots_dynamic_kick)/config/kick_config.yaml" />
    </node>
    
    <rosparam  command="load" file="$(find bitbots_quintic_walk)/config/walk_config.yaml"/>
    <node name="walking" pkg="bitbots_quintic_walk" type="WalkNode" output="screen"/>
    
    <node name="dynup" pkg="thmos_dynup" type="DynupNode" output="screen" />
</launch>
